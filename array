#include <stdio.h>
#include<stdlib.h>
int main()
{
    int element[3][3]={1,2,3,4,5,6,7,8,9};
    int i,j,k,sum;
    k=element[i][j];
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
          printf("%d\t",element[i][j]);  
        }
        printf("\n");

    }
    sum=0;
    for(k=0;k<3;k++)
    {
        sum=sum+element[k][k];
    }
       printf("sum:%d",sum);
    return 0;
}
